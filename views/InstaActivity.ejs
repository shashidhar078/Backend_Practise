<%- include("includes/sample") %>
<body>
    <h1>This page belongs to @<%= username %></h1>
    <button>Follow</button>
    <button>Message</button>
    <h3>Accounts that follow you : </h3>
    <% for(let name of followers) {%>
        <li><%= name%></li>
    <% }%>
    <h2>Followers : <%= data.followers %> &nbsp;&nbsp;&nbsp; following : <%= data.following %></h2> 
    <hr />
    <% for(let post of data.posts)
    { %>
        <img src="<%= post.image %>" alt="posts">
        <h2>likes : <%= post.likes %> &nbsp;&nbsp;&nbsp; comments : <%= post.comments %></h2>
    <% } %>
</body>
<script src="/app.js"></script>
</html>